{
	"manifest_version":	2,
	"name":	"SoundWave!",
	"version":	"1.0",
	"description":	"Control Chrome Browser with your hands using Doppler Effect!",
	"browser_action":
	{
		"default_icon":	"off"
	},
	"content_scripts":
	[
		{
			"matches":	["<all_urls>"],
			"js":	["soundWave.js", "gestureMapper.js", "browserAction.js"]
		}
	],
	"background":
	{
		"scripts":	["background.js"]
	},
	"permissions": [
		"activeTab",
		"<all_urls>",
		"sessions"
	]
}
